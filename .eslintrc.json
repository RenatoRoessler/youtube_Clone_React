{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "prettier",
        "prettier/react"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "prettier/prettier" : "error",
        "react/jsx-filename-extension": [1, {"extensons": [".js", ".jsx"]}],
        "react/props-types": "ignore",
        "custom": "ignore",
        "exceptions": [""],
        "import/no-unresolved": "off"
    }
}
